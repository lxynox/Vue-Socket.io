{
	"name": "vue-socket",
	"version": "0.0.1",
	"description": "socket.io(client) plugin for vue",
	"main": "index.js",
	"scripts": {
		"build": "webpack --progress --hide-modules",
		"lint": "xo",
		"test": "mocha"
	},
	"xo": {
		"esnext": true,
		"space": false,
		"semicolon": false,
		"env": [
			"browser",
			"node",
			"mocha"
		],
		"overrides": [
			{
				"files": "test/**",
				"esnext": false
			}
		],
		"ignore": [
			"./index.js",
			"src/**",
			"dist/**"
		]
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"keywords": [
		"vuejs",
		"socket.io-client",
