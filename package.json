	"author": "lxynox",
	"license": "MIT",
	"bugs": {
		"url": ""
	},
	"homepage": "",
	"dependencies": {
		"socket.io-client": "^1.4.6"
	},
	"devDependencies": {
		"babel-cli": "^6.11.4",
		"babel-loader": "^6.0.0",
		"babel-preset-es2015": "^6.3.13",
		"mocha": "^3.2.0",
		"sinon": "^1.17.7",
		"socket.io": "^1.7.3",
		"webpack": "^2.2.0-rc.3",
		"xo": "^0.17.1"
	},
	"peerDependencies": {
		"vue": "^2.2.2"
